{
  "name": "v2rayA Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "golang.go",
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode"
  ],
  "runArgs": [
    "--privileged"
  ],
  "postCreateCommand": "yarn --cwd gui install && yarn --cwd gui build",
  "remoteUser": "vscode"
}
